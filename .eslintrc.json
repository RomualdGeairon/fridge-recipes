{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": ["warn", {
            "ignoreComments": true,
            "ignoreTrailingComments": true,
            "code": 120
            }],
        "react/no-unescaped-entities": "off"
    }
}
